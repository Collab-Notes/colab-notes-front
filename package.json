{
  "name": "colab-notes-frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev --dotenv .env.stage",
    "build": "node --max-old-space-size=4096 node_modules/nuxt/bin/nuxt.mjs build",
    "build:icons": "tsx src/vendor/materio/plugins/iconify/build-icons.ts",
    "postinstall": "nuxt prepare && npm run build:icons ",
    "generate": "nuxt generate",
    "lint:check": "eslint ./src --max-warnings 0 --config eslint.config.js",
    "lint:fix": "eslint ./src --config eslint.config.js --fix",
    "lint-style:check": "stylelint ./src '**/*.{css,scss,sass,vue}' --max-warnings 0 --config stylelint.config.js",
    "lint-style:fix": "stylelint ./src '**/*.{css,scss,sass,vue}' --config stylelint.config.js --fix",
    "format:check": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "@floating-ui/dom": "1.6.13",
    "@iconify-json/bxl": "^1.2.2",
    "@stylistic/stylelint-config": "^2.0.0",
    "@tailwindcss/vite": "^4.0.17",
    "@vee-validate/nuxt": "^4.15.0",
    "@vee-validate/zod": "^4.15.0",
    "@vueuse/core": "^13.0.0",
    "@vueuse/math": "^13.0.0",
    "apexcharts": "^4.5.0",
    "nuxt": "3.16.1",
    "pinia": "^3.0.1",
    "prismjs": "^1.30.0",
    "roboto-fontface": "^0.10.0",
    "tailwindcss": "^4.0.17",
    "vee-validate": "^4.15.0",
    "vue-flatpickr-component": "12.0.0",
    "vue-prism-component": "^2.0.0",
    "vue3-apexcharts": "1.8.0",
    "vue3-perfect-scrollbar": "^2.0.0",
    "vuetify": "3.7.19",
    "webfontloader": "^1.6.28",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@antfu/utils": "^9.1.0",
    "@iconify-json/ri": "^1.2.5",
    "@iconify/tools": "^4.1.2",
    "@iconify/utils": "^2.3.0",
    "@iconify/vue": "4.3.0",
    "@nuxt/eslint": "^1.2.0",
    "@nuxtjs/device": "^3.2.4",
    "@pinia/nuxt": "^0.10.1",
    "@types/node": "^22.13.13",
    "@types/webfontloader": "^1.6.38",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vueuse/nuxt": "^13.0.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-vue": "^10.0.0",
    "lefthook": "^1.11.5",
    "postcss-html": "^1.8.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.5.3",
    "sass": "~1.86.0",
    "sass-embedded": "^1.86.0",
    "stylelint": "16.17.0",
    "stylelint-codeguide": "2.0.0",
    "stylelint-config-idiomatic-order": "10.0.0",
    "stylelint-config-standard-scss": "14.0.0",
    "stylelint-use-logical-spec": "5.0.1",
    "tsx": "^4.19.3",
    "type-fest": "^4.38.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.3",
    "vite-plugin-vuetify": "2.1.0",
    "vite-svg-loader": "^5.1.0"
  },
  "resolutions": {
    "postcss": "^8",
    "stylelint-order": "6.0.3",
    "@types/video.js": "^7"
  },
  "overrides": {
    "postcss": "^8",
    "stylelint-order": "6.0.3",
    "@types/video.js": "^7"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "lefthook",
      "vue-demi"
    ]
  },
  "packageManager": "pnpm@10.7.0"
}
